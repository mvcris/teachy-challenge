{
  "extends": ["next/core-web-vitals", "airbnb", "plugin:cypress/recommended"],
  "rules": {
    "indent": ["error", 2],
    "import/prefer-default-export":"off",
    "react/jsx-filename-extension": "off",
    "react/react-in-jsx-scope":"off",
    "react/require-default-props": "off",
    "no-unused-vars":"off",
    "no-shadow": "off",
    "array-callback-return": "off",
    "consistent-return":"off",
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
   ]
  }
}
